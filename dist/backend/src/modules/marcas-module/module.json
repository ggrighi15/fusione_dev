    "documentos": {
      "armazenamento": {
        "local": "/uploads/marcas",
        "tipos_permitidos": [
          "pdf", "doc", "docx", "jpg", "jpeg", "png", "gif", "tiff",
          "zip", "rar", "xml", "txt", "rtf"
        ],
        "tamanho_maximo_mb": 50,
        "versionamento": true,
        "backup_automatico": true
      },
      "assinatura_digital": {
        "obrigatoria": false,
        "tipos_aceitos": ["ICP-Brasil", "Adobe Sign", "DocuSign"],
        "validacao_automatica": true
      },
      "marca_dagua": {
        "aplicar": true,
        "texto": "CONFIDENCIAL - {EMPRESA}",
        "posicao": "diagonal",
        "opacidade": 0.3
      },
      "ocr": {
        "habilitado": true,
        "idiomas": ["por", "eng", "spa"],
        "indexacao_automatica": true
      }
    },
    "integracao": {
      "inpi_api": {
        "habilitada": true,
        "url_base": "https://gru.inpi.gov.br/pePI/api",
        "timeout_segundos": 30,
        "retry_tentativas": 3,
        "cache_duracao_horas": 6
