{
  "name": "fusione-core-system",
  "version": "1.0.0",
  "description": "Sistema central Fusione - Core system para integração e gerenciamento de módulos",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:ai": "nodemon --inspect=0.0.0.0:9230 src/index.js --module=ai-module",
    "dev:contencioso": "nodemon --inspect=0.0.0.0:9231 src/index.js --module=contencioso-module",
    "dev:contencioso:watch": "nodemon --inspect=0.0.0.0:9231 --watch src/modules/contencioso-module src/index.js --module=contencioso-module",
    "dev:data-analysis": "nodemon --inspect=0.0.0.0:9232 src/index.js --module=data-analysis-module",
    "dev:auth": "nodemon --inspect=0.0.0.0:9233 src/index.js --module=auth-module",
    "dev:reports": "nodemon --inspect=0.0.0.0:9234 src/index.js --module=reports-module",
    "dev:notification": "nodemon --inspect=0.0.0.0:9235 src/index.js --module=notification-module",
    "dev:dashboard": "nodemon --inspect=0.0.0.0:9236 src/dashboard/index.js",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js",
    "test": "jest",
    "test:ai": "jest --testPathPattern=ai-module",
    "test:contencioso": "jest --testPathPattern=contencioso-module",
    "test:data-analysis": "jest --testPathPattern=data-analysis-module",
    "test:auth": "jest --testPathPattern=auth-module",
    "test:reports": "jest --testPathPattern=reports-module",
    "test:notification": "jest --testPathPattern=notification-module",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "node scripts/build.js",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "install:frontend": "cd frontend && npm install",
    "start:frontend": "cd frontend && npm start",
    "lint": "eslint src/**/*.js",
    "lint:ai": "eslint src/modules/ai-module/**/*.js",
    "lint:contencioso": "eslint src/modules/contencioso-module/**/*.js",
    "lint:data-analysis": "eslint src/modules/data-analysis-module/**/*.js",
    "lint:auth": "eslint src/modules/auth-module/**/*.js",
    "lint:reports": "eslint src/modules/reports-module/**/*.js",
    "lint:notification": "eslint src/modules/notification-module/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:ai": "prettier --write src/modules/ai-module/**/*.js",
    "format:contencioso": "prettier --write src/modules/contencioso-module/**/*.js",
    "format:data-analysis": "prettier --write src/modules/data-analysis-module/**/*.js",
    "format:auth": "prettier --write src/modules/auth-module/**/*.js",
    "format:reports": "prettier --write src/modules/reports-module/**/*.js",
    "format:notification": "prettier --write src/modules/notification-module/**/*.js",
    "docker:build": "docker build -t fusione-core .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t fusione-core:dev .",
    "docker:run": "docker-compose up -d",
    "docker:run:dev": "docker-compose --profile development up -d",
    "docker:run:contencioso": "docker-compose up contencioso-dev",
    "docker:run:modules": "docker-compose -f docker-compose.modules.yml up",
    "docker:run:ai": "docker-compose -f docker-compose.modules.yml --profile ai-development up",
    "docker:run:data-analysis": "docker-compose -f docker-compose.modules.yml --profile data-analysis-development up",
    "docker:run:auth": "docker-compose -f docker-compose.modules.yml --profile auth-development up",
    "docker:run:reports": "docker-compose -f docker-compose.modules.yml --profile reports-development up",
    "docker:run:notification": "docker-compose -f docker-compose.modules.yml --profile notification-development up",
    "docker:run:dashboard": "docker-compose -f docker-compose.modules.yml --profile dashboard up",
    "docker:stop": "docker-compose down",
    "docker:stop:modules": "docker-compose -f docker-compose.modules.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:contencioso": "docker-compose logs -f contencioso-dev",
    "docker:logs:modules": "docker-compose -f docker-compose.modules.yml logs -f"
  },
  "keywords": [
    "fusione",
    "core-system",
    "modular",
    "integration",
    "management"
  ],
  "author": "Gustavo Righi <gustavorighi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "geoip-lite": "^1.4.10",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "nodemailer": "^7.0.5",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "useragent": "^2.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^27.5.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
